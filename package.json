{
  "name": "rive",
  "version": "3.0.1",
  "description": "Effortless React Library Development Experience",
  "keywords": [
    "react",
    "typescript",
    "library",
    "component",
    "vite",
    "document"
  ],
  "homepage": "https://github.com/guoyunhe/rive#readme",
  "bugs": {
    "url": "https://github.com/guoyunhe/rive/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guoyunhe/rive.git"
  },
  "funding": "https://github.com/sponsors/guoyunhe",
  "license": "GPL-3.0",
  "author": "Guo Yunhe",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": "dist/rive.js",
  "files": [
    "dist",
    "globals.d.ts",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "rive build",
    "build:watch": "rive build --watch",
    "ci:eslint": "eslint -f json src -o ./.ci/eslint.json",
    "lint": "eslint .",
    "lint:fix": "prettier --write . && eslint --fix .",
    "prepare": "husky",
    "start": "rive start",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest"
  },
  "lint-staged": {
    "*.{cjs,cts,js,jsx,mjs,mts,ts,tsx,vue}": "eslint --fix",
    "*.{cjs,css,cts,html,js,json,jsx,less,md,mjs,mts,scss,ts,tsx,vue,yaml,yml}": "prettier --write"
  },
  "prettier": "prettier-config-ali",
  "dependencies": {
    "@mdx-js/react": "^3.1.0",
    "@mdx-js/rollup": "^3.1.0",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "@vitest/ui": "^2.1.8",
    "chalk": "^5.4.1",
    "chokidar": "^3.6.0",
    "commander": "^12.1.0",
    "eslint": "^9.17.0",
    "eslint-config-rive": "^3.1.1",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "happy-dom": "^16.0.1",
    "i18n": "^0.15.1",
    "json5": "^2.2.3",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-config-rive": "^1.0.0",
    "react": "^18.3.1",
    "react-doc-ui": "^2.3.0",
    "react-docgen-typescript": "^2.2.2",
    "react-dom": "^18.3.1",
    "recma-export-filepath": "^1.1.0",
    "recma-mdx-displayname": "^0.4.1",
    "rehype-mdx-code-imports": "^0.4.3",
    "rehype-mdx-code-props": "^2.0.0",
    "rehype-mdx-import-media": "^1.2.0",
    "rehype-mdx-title": "^3.1.0",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.0",
    "remark-mdx-frontmatter": "^4.0.0",
    "simple-git": "^3.27.0",
    "stylelint": "^16.12.0",
    "stylelint-config-rive": "^2.0.0",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.7.2",
    "vite": "^6.0.6",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
  },
  "devDependencies": {
    "@mdx-js/react": "^3.1.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.1.0",
    "@types/eslint": "^9.6.1",
    "@types/fs-extra": "^11.0.4",
    "@types/i18n": "^0.13.12",
    "@types/node": "^20.17.30",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "@vitest/ui": "^3.1.1",
    "eslint": "^9.24.0",
    "eslint-config-ali": "^16.1.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "prettier-config-ali": "^1.3.2",
    "prettier-plugin-packagejson": "^2.5.6",
    "react": "^18.3.1",
    "react-doc-ui": "^2.3.0",
    "react-dom": "^18.3.1",
    "rive": "^3.0.1",
    "tsx": "^4.19.2",
    "typescript": "^5.8.3",
    "vitest": "^3.1.1"
  },
  "rive": {
    "template": "cli",
    "doc": {
      "basename": "/rive/"
    }
